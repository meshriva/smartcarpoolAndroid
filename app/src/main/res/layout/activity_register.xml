<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.ibm.techathon.elven.smartpool.activity.RegisterActivity"
    android:id="@+id/view_register">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:hint="@string/email"
        android:fontFamily="@string/font_family"
        android:id="@+id/editText_register_email"
        android:layout_toEndOf="@+id/imageView_register_email"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView_register_email"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="@dimen/list_image_width"
        android:layout_height="@dimen/list_image_height"
        android:id="@+id/imageView_register_email"
        android:src="@drawable/ic_mail_96"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:hint="@string/user_name"
        android:fontFamily="@string/font_family"
        android:id="@+id/editText_register_user_name"
        android:layout_toEndOf="@+id/imageView_register_user_name"
        android:layout_toRightOf="@+id/imageView_register_user_name"
        android:layout_below="@+id/editText_register_email"
        android:layout_alignLeft="@+id/editText_register_email"
        android:layout_alignStart="@+id/editText_register_email"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/registeration_edit_margin_top"/>

    <ImageView
        android:layout_width="@dimen/list_image_width"
        android:layout_height="@dimen/list_image_height"
        android:id="@+id/imageView_register_user_name"
        android:src="@drawable/ic_user_96"
        android:layout_alignBottom="@+id/editText_register_user_name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:hint="@string/password"
        android:fontFamily="@string/font_family"
        android:id="@+id/editText_register_password"
        android:layout_toEndOf="@+id/imageView_register_password"
        android:layout_toRightOf="@+id/imageView_register_password"
        android:layout_below="@+id/editText_register_user_name"
        android:layout_alignLeft="@+id/editText_register_user_name"
        android:layout_alignStart="@+id/editText_register_user_name"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/registeration_edit_margin_top"/>

    <ImageView
        android:layout_width="@dimen/list_image_width"
        android:layout_height="@dimen/list_image_height"
        android:id="@+id/imageView_register_password"
        android:src="@drawable/ic_key_96"
        android:layout_alignBottom="@+id/editText_register_password"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:hint="@string/confirm_password"
        android:fontFamily="@string/font_family"
        android:id="@+id/editText_register_confirm_password"
        android:layout_toEndOf="@+id/imageView_register_confirm_password"
        android:layout_toRightOf="@+id/imageView_register_confirm_password"
        android:layout_below="@+id/editText_register_password"
        android:layout_alignLeft="@+id/editText_register_password"
        android:layout_alignStart="@+id/editText_register_password"
        android:layout_alignRight="@+id/editText_register_password"
        android:layout_alignEnd="@+id/editText_register_password"
        android:layout_marginTop="@dimen/registeration_edit_margin_top"/>

    <ImageView
        android:layout_width="@dimen/list_image_width"
        android:layout_height="@dimen/list_image_height"
        android:id="@+id/imageView_register_confirm_password"
        android:src="@drawable/ic_key_96"
        android:layout_alignBottom="@+id/editText_register_confirm_password"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:hint="@string/phone_number"
        android:fontFamily="@string/font_family"
        android:id="@+id/editText_register_phone_number"
        android:layout_toEndOf="@+id/imageView_register_phone_number"
        android:layout_toRightOf="@+id/imageView_register_phone_number"
        android:layout_below="@+id/editText_register_confirm_password"
        android:layout_alignLeft="@+id/editText_register_confirm_password"
        android:layout_alignStart="@+id/editText_register_confirm_password"
        android:layout_alignRight="@+id/editText_register_confirm_password"
        android:layout_alignEnd="@+id/editText_register_confirm_password"
        android:layout_marginTop="@dimen/registeration_edit_margin_top"/>

    <ImageView
        android:layout_width="@dimen/list_image_width"
        android:layout_height="@dimen/list_image_height"
        android:id="@+id/imageView_register_phone_number"
        android:src="@drawable/ic_phone_96"
        android:layout_alignBottom="@+id/editText_register_phone_number"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <AutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:hint="@string/current_location"
        android:fontFamily="@string/font_family"
        android:id="@+id/editText_register_current_location"
        android:layout_toEndOf="@+id/imageView_register_current_location"
        android:layout_toRightOf="@+id/imageView_register_current_location"
        android:layout_below="@+id/editText_register_phone_number"
        android:layout_alignLeft="@+id/editText_register_phone_number"
        android:layout_alignStart="@+id/editText_register_phone_number"
        android:layout_alignRight="@+id/editText_register_phone_number"
        android:layout_alignEnd="@+id/editText_register_phone_number"
        android:layout_marginTop="@dimen/registeration_edit_margin_top"/>

    <ImageView
        android:layout_width="@dimen/list_image_width"
        android:layout_height="@dimen/list_image_height"
        android:id="@+id/imageView_register_current_location"
        android:src="@drawable/ic_compass_96"
        android:layout_alignBottom="@+id/editText_register_current_location"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:hint="@string/vechile_registeration_number"
        android:fontFamily="@string/font_family"
        android:id="@+id/editText_register_vechile_registeration_number"
        android:layout_toEndOf="@+id/imageView_register_vechile_registeration_number"
        android:layout_toRightOf="@+id/imageView_register_vechile_registeration_number"
        android:layout_below="@+id/editText_register_current_location"
        android:layout_alignLeft="@+id/editText_register_current_location"
        android:layout_alignStart="@+id/editText_register_current_location"
        android:layout_alignRight="@+id/editText_register_current_location"
        android:layout_alignEnd="@+id/editText_register_current_location"
        android:layout_marginTop="@dimen/registeration_edit_margin_top"/>

    <ImageView
        android:layout_width="@dimen/list_image_width"
        android:layout_height="@dimen/list_image_height"
        android:id="@+id/imageView_register_vechile_registeration_number"
        android:src="@drawable/ic_car_96"
        android:layout_alignBottom="@+id/editText_register_vechile_registeration_number"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="?android:textAppearanceSmall"
        android:fontFamily="@string/font_family"
        android:textSize="@dimen/registeraton_button_text_size"
        android:text="@string/register_me"
        android:id="@+id/button_register_me"
        android:ems="10"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/imageView_register_vechile_registeration_number"
        android:layout_toRightOf="@+id/imageView_register_vechile_registeration_number"
        android:layout_marginBottom="@dimen/registeraton_button_margin_top"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
